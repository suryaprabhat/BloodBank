{
    "version": 2,
    "builds": [
      {
        "src": "server/api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/server/api/$1" },
      { "src": "/(.*)", "dest": "/frontend/$1" }
    ]
  }
  